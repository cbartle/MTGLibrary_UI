{
  "expo": {
    "name": "MTGLibraryApp",
    "slug": "mtg-library-app",
    "version": "1.0.0",
    "sdkVersion": "50.0.0",
    "platforms": ["android"],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.cbartle.mtglibraryapp"
    }
  }
}
